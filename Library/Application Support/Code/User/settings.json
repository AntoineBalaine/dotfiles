{
  "workbench.iconTheme": "material-icon-theme",
  "editor.lineNumbers": "relative",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "editor.wordWrap": "off",
  "editor.cursorBlinking": "solid",
  "vim.useSystemClipboard": true,
  "vim.smartRelativeLine": true,
  "vim.leader": "<space>",
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.blockComment"]
    },
    {
      "before": ["<leader>", "d", "p"],
      "commands": ["editor.debug.action.selectionToRepl"]
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-v>"],
      "commands": ["editor.action.clipboardPasteAction"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["K"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["g", "r"],
      "commands": ["gitlens.diffWithRevisionFrom"],
      "args": "dev"
    },
    {
      "before": ["g", "p"],
      "commands": ["svelte.typescript.findComponentReferences"]
    },
    {
      "before": ["<leader>", "d", "t"],
      "commands": ["workbench.debug.viewlet.action.toggleBreakpointsActivatedAction"]
    },
    {
      "before": ["J"],
      "after": ["<leader>", "<leader>", "2", "F"]
    },
    {
      "before": ["j"],
      "after": ["<leader>", "<leader>", "2", "s"]
    },
    { "before": ["<leader>", "/"], "commands": ["editor.action.commentLine"] },
    {
      "before": ["R"],
      "after": ["v", "i", "w", "p", "y", "i", "w"]
    },
    {
      "before": ["<leader>", "q"],
      "after": ["/", "F", "C", "<CR>", "0", "W", "g", "d"]
    },
    {
      "before": ["<leader>", "h"],
      "after": ["y", "i", "w", "/", "<C-v>", "<CR>", "/", "F", "C", "<CR>", "0", "W", "g", "d"]
    },
    {
      "before": ["<leader>", "f"],
      "after": ["/", "up", "up", "<CR>", "n", "g", "d"]
    }
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "debug.javascript.breakOnConditionalError": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.showMarksInGutter": true,
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.insertEnclosingFunction": false,
  "turboConsoleLog.logMessagePrefix": "",
  "turboConsoleLog.delimiterInsideMessage": "",
  "turboConsoleLog.insertEnclosingClass": false,
  "git.autofetch": true,
  "extensions.ignoreRecommendations": true,
  "githubPullRequests.fileListLayout": "tree",
  "githubPullRequests.pullBranch": "never",
  "javascript.preferences.useAliasesForRenames": false,
  "typescript.preferences.useAliasesForRenames": false,
  "git.confirmSync": false,
  "eslint.lintTask.enable": true,
  "git.mergeEditor": false,
  "gitlens.changes.toggleMode": "window",
  "vim.easymotion": true,
  "workbench.editor.pinnedTabSizing": "shrink",
  "tabsort.sortByFullPath": false,
  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.note-new-transformation": false,
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": true,
  "debug.allowBreakpointsEverywhere": true,
  "editor.accessibilitySupport": "off",
  "debug.onTaskErrors": "debugAnyway",
  "thunder-client.httpLibrary": "axios",
  "prettier.printWidth": 153,
  "editor.inlineSuggest.enabled": true,
  "workbench.fontAliasing": "antialiased",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": false,
    "typescriptreact": true
  },
  "workbench.startupEditor": "none",
  "typeChallenges.workspaceFolder": "/Users/abalaine/.typeChallenges",
  "snyk.advanced.cliPath": "/Users/abalaine/Library/Application Support/snyk-ls/snyk-macos",
  "snyk.yesWelcomeNotification": false,
  "snyk.trustedFolders": [
    "/Users/abalaine/Documents/Ryerson/web-components-common-ui",
    "/Users/abalaine/Documents/Ryerson/web-components-common-ui",
    "/Users/abalaine/Documents/Ryerson/web-store-ui/application"
  ],
  "errorLens.excludeBySource": ["ts(2304)"],
  "workbench.colorTheme": "Black",
  "workbench.editor.wrapTabs": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.openRepositoryInParentFolders": "always",
  "window.zoomLevel": 2,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "editor.suggest.showWords": false,
  "reascript-docs.reaImGuiVersion": "<0.9",
  "github.copilot.editor.enableAutoCompletions": true,
  "zig.path": "zig",
  "go.formatTool": "gofmt",
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "github.copilot.preferredAccount": "266836_nttds",
  "github.copilot.advanced": {},
  "zig.zls.enabled": "on",
  "[typst]": {
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
  },
  "[typst-code]": {
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
  },
  "sonarlint.connectedMode.connections.sonarqube": [
    {
      "serverUrl": "https://sonarqube.io.pge.com",
      "connectionId": "https-sonarqube-io-pge-com-"
    }
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.blame.editorDecoration.enabled": true,
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
